<%= render 'components/default_header' %>

<h1>Create a new Tent Place</h1>
<p>Find me in app/views/tent/new.html.erb</p>

<%= simple_form_for @tent do |f| %>
  <%= f.input :title %>
  <%= f.input :capacity %>
  <%= f.input :description %>
  <%= f.input :location %>
  <%= f.input :price %>
  <%= f.input :wildlife %>
  <%= f.input :photo %>
  <%= f.button :submit %>
<% end %>

