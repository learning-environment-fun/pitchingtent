<div class="content-create-tent">



<div class="tent-wrapper">
  <div class="tent-form">
  <h1>Rent your Tent!</h1>
<%= simple_form_for @tent do |f| %>
<%= f.input :title %>
<%= f.input :capacity %>
<%= f.input :description %>
<%= f.input :location %>
<%= f.input :price %>
<%= f.input :wildlife %>
<%= f.input :photo %>
<%= f.input :photo_cache, as: :hidden %>
<%= f.button :submit %>
<% end %>
</div>
</div>




</div>





